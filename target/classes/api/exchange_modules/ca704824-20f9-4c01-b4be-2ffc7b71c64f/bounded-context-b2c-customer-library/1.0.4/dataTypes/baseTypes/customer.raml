#%RAML 1.0 DataType
displayName: Customer
type: object
properties:
  accaId:
    type: string
    required: true
  forename:
    type: string
    required: false
  surname:
    type: string
    required: false
  customerType:
    enum:
      - student
      - member
      - affiliate
    required: false
  country:
    type: string
    required: false
  onlineCountry:
    type: boolean
    required: false
